<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="E:\workspace\intellij_idea\mysql-connector-java-5.1.46.jar" />
    <context id="XHTables" targetRuntime="MyBatis3">
        <!--数据库连接-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/bsdb?useUnicode=true&amp;characterEncoding=UTF-8"
                        userId="root"
                        password="root">
        </jdbcConnection>
        <!--model 和 example位置-->
        <javaModelGenerator targetPackage="entity" targetProject="E:\workspace\intellij_idea\cyx-generator\src\main\java\test">
        </javaModelGenerator>
        <!--mapping文件位置-->
        <sqlMapGenerator targetPackage="dao"  targetProject="E:\workspace\intellij_idea\cyx-generator\src\main\java\test">
        </sqlMapGenerator>
        <!--XML位置-->
        <javaClientGenerator type="mapping" targetPackage="dao" targetProject="E:\workspace\intellij_idea\cyx-generator\src\main\java\test">
        </javaClientGenerator>
        <!--要生成的表名-->
        <table tableName="user_info" domainObjectName="UserInfo" />
        <table tableName="person_info" domainObjectName="PersonInfo" />
    </context>
</generatorConfiguration>